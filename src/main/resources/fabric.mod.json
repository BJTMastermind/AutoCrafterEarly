{
    "schemaVersion": 1,
    "id": "crafter",
    "version": "${version}",
    "name": "Autocrafter Early",
    "description": "Backports the crafter block from 1.21 to 1.20.2.",
    "authors": [
        "QuackImpala7321",
        "BJTMastermind"
    ],
    "contact": {
        "sources": "https://github.com/BJTMastermind/AutoCrafterEarly"
    },
    "license": "MIT",
    "icon": "assets/crafter/icon.png",
